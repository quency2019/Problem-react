{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "skipLibCheck": true,
    "allowJs": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "strict": true,
    "removeComments": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@tmp/*": [
        "src/pages/.umi/*"
      ]
    },
    "allowSyntheticDefaultImports": true,
  }
}